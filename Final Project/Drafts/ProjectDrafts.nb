Cell[BoxData[{
 RowBox[{
  RowBox[{"$cachePath", "=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"folder", "=", 
       RowBox[{"FileNameJoin", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"NotebookDirectory", "[", "]"}], ",", "\"\<cache\>\""}], 
         "}"}], "]"}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"Quiet", "@", 
       RowBox[{"CreateDirectory", "[", "folder", "]"}]}], ";", 
      "\[IndentingNewLine]", "folder"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{"SetAttributes", "[", 
  RowBox[{"keyValueStore", ",", "HoldRest"}], "]"}], "\n", 
 RowBox[{
  RowBox[{"keyValueStore", "[", 
   RowBox[{"expr_", ",", 
    RowBox[{"default_:", "None"}]}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"path", "=", 
      RowBox[{"FileNameJoin", "[", 
       RowBox[{"{", 
        RowBox[{"$cachePath", ",", 
         RowBox[{
          RowBox[{"IntegerString", "[", 
           RowBox[{
            RowBox[{"Hash", "[", "expr", "]"}], ",", "36"}], "]"}], "<>", 
          "\"\<.wxf\>\""}]}], "}"}], "]"}]}], "}"}], ",", 
    RowBox[{"Replace", "[", 
     RowBox[{
      RowBox[{"Quiet", "@", 
       RowBox[{"Import", "[", 
        RowBox[{"path", ",", "\"\<WXF\>\""}], "]"}]}], ",", 
      RowBox[{
       RowBox[{"_", "?", "FailureQ"}], "\[RuleDelayed]", 
       RowBox[{"With", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"evaluated", "=", "default"}], "}"}], ",", 
         RowBox[{
